# Ethereum private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URL (optional, defaults to https://eth.llamarpc.com)
RPC_URL=https://eth.llamarpc.com

# Action to perform: info, supply, withdraw, borrow, repay
ACTION=info

# Amount in USDC (required for supply, withdraw, borrow, repay actions)
AMOUNT=100.0